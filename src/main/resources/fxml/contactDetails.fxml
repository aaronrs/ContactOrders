<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="218.0" prefWidth="354.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.astechdesign.clients.gui.controllers.ContactDetailsController">
   <children>
      <TextField fx:id="address" promptText="Address" />
      <TextField fx:id="postcode" promptText="Postcode" />
      <TextField fx:id="telephone" prefHeight="25.0" prefWidth="178.0" promptText="Telephone" />
      <Button fx:id="updateDetailsBtn" mnemonicParsing="false" onAction="#updateDetails" text="UPDATE" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
